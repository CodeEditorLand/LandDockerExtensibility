{
	"name": "@microsoft/vscode-container-client",
	"version": "0.0.1",
	"private": false,
	"description": "Extensibility model for implementing container runtime providers (shared by VS and VS Code)",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandDockerExtensibility#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandDockerExtensibility/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandDockerExtensibility.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "lib/index.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build": "tsc",
		"clean": "tsc --build --clean",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"testreport": "mocha --reporter mocha-junit-reporter",
		"testwatch": "mocha --watch"
	},
	"mocha": {
		"extension": [
			"ts"
		],
		"require": [
			"ts-node/register",
			"./src/test/tsconfig-paths-bootstrap.js"
		],
		"spec": [
			"src/test/**/*.test.ts"
		],
		"watch-files": [
			"src/**/*.ts"
		]
	},
	"dependencies": {
		"dayjs": "^1.11.2",
		"tree-kill": "^1.2.2"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/chai": "^4.3.0",
		"@types/node": "18.x",
		"chai": "^4.3.6",
		"tsconfig-paths": "^4.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
